{
  "name": "mg-bdd",
  "version": "1.0.0",
  "main": "index.js",
 "scripts": {
  "test:all": "npx cucumber-js --config config-directory/cucumber.json ./tests/features",
  "test:web": "npx cucumber-js --tags @web --config config-directory/cucumber.json --format json:reports/web-report.json --format html:reports/web-report.html",
  "test:api": "npx cucumber-js --tags @api --parallel 1 --config config-directory/cucumber.json --format json:reports/api-report.json --format html:reports/api-report.html",
  "test:ok": "concurrently \"npm run test:web\" \"npm run test:api\""
},
  "keywords": [],
  "author": "Milind G",
  "license": "ISC",
  "description": "Overview :\r This framework combines Playwright for browser automation and Cucumber.js for (BDD), enabling seamless web and API testing. The modular design ensures flexibility and scalability, offering robust testing for web applications and APIs.",
  "devDependencies": {
    "@cucumber/cucumber": "^11.1.1",
    "@cucumber/html-formatter": "^21.7.0",
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.2",
    "cucumber": "^6.0.7",
    "playwright": "^1.49.1"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@slack/web-api": "^7.8.0",
    "axios": "^1.7.9",
    "cucumber-html-reporter": "^6.0.0",
    "dotenv": "^16.4.7",
    "expect": "^29.7.0",
    "form-data": "^4.0.1",
    "fs": "^0.0.1-security",
    "log4js": "^6.9.1",
    "mg-bdd": "file:",
    "path": "^0.12.7",
    "qs": "^6.13.1"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milind1213/MG-BDD.git"
  },
  "type": "commonjs",
  "bugs": {
